+static void setup_dops(struct super_block *sb)
+{
+       if (SDFAT_SB(sb)->options.casesensitive == 0)
+               sb->s_d_op = &sdfat_ci_dentry_ops;
+       else
+               sb->s_d_op = &sdfat_dentry_ops;
+}

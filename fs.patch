+static struct sdfat_attr sdfat_attr_##name = __ATTR(name, mode, show, store)
+
+struct sdfat_attr {
+	struct attribute attr;
+	ssize_t (*show)(struct sdfat_sb_info *, char *);
+	ssize_t (*store)(struct sdfat_sb_info *, const char *, size_t);
+};

+static void init_once(void *foo)
+{
+	struct sdfat_inode_info *ei = (struct sdfat_inode_info *)foo;
+
+	INIT_HLIST_NODE(&ei->i_hash_fat);
+	inode_init_once(&ei->vfs_inode);
+}
